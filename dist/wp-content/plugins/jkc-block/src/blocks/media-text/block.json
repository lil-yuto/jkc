{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "jkc-block/media-text",
  "version": "0.1.0",
  "title": "画像/テキスト",
  "category": "jkc-block",
  "icon": {
    "src": "block-default",
    "foreground": "#0A6934"
  },
  "description": "画像/テキスト",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "role": "content"
    },
    "placeholder": {
      "type": "string",
      "default": "テキスト"
    },
    "imageId": {
      "type": "number",
      "default": 0
    },
    "imageSrc": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    },
    "imageAlt": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "alt"
    },
    "imageWidth": {
      "type": "number",
      "default": 50
    },
    "imagePosition": {
      "type": "string",
      "default": "left"
    }
  },
  "textdomain": "jkc-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
